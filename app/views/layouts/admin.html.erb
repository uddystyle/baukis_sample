<!DOCTYPE html>
<html>
<head>
  <title><%= document_title %></title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'admin', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'admin', 'data-turbolinks-track' => true %>
</head>

<body>
<div id="wrapper">
  <%= render 'admin/shared/header' %>
  <div id='container'>
    <%= yield %>
  </div>
  <%= render 'shared/footer' %>
</div>
</body>
</html>
